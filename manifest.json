{
  "author": "Maros Meciar",
  "name": "VSB E-mail Notifier",
  "description": "Notifies you about new e-mails in your VSB mailbox.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "alarms", "notifications"],
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  }
}
